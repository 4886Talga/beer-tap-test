{
    "extends" : [
        "airbnb",
        "prettier"
    ],
    "env": {
        "jest": true,
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "rules": {
        "valid-typeof": 1,
        "react/prop-types": 0,
        "no-mixed-spaces-and-tabs": 0,
        "indent":0,
        "react/jsx-indent": 0,
        "linebreak-style": 0,
        "quotes":0,
        "semi": 1,
        "no-unused-vars": 0,
        "no-undef": 0,
        "no-console": 0,
        "comma-dangle": 0,
        "react/jsx-uses-vars": 1,
        "react/display-name": 0,
        "react/jsx-key": 1,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}